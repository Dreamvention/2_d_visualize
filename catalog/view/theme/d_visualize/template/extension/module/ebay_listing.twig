<div class="vz-extension-ebay-listing">
  <div class="heading-h3 vz-extension-ebay-listing__heading">{{ heading_title }}</div>
  <div class="row">
      {% for product in products %}
        <div class="product-layout col-lg-3 col-md-6 col-xs-12">
            {% include component.d_product_thumb.template with component.d_product_thumb %}
        </div>
      {% endfor %}
  </div>
  <img src="{{tracking_pixel}}" height="0" width="0" />
</div>

