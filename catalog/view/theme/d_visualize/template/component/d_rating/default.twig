<div class="rating">
	<input value="{{ rating }}" name="rating" hidden
		   class="rating-loading" data-filled="fas fa-star" data-empty="fas fa-star empty"
            {% if static_rating %} data-readonly{% endif %}>
</div>
<script>
$(document).ready(function () {
	$('input[name=\'rating\']').rating();
})

</script>