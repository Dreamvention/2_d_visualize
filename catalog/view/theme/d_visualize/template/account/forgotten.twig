{{header}}
{% include 'd_theme_twig/template/partial/d_breadcrumb.twig' %}
    {% include 'd_theme_twig/template/partial/d_layout_open.twig' %}
      <h1 class="h1">{{heading_title}}</h1>
      <p>{{text_email}}</p>
      <form action="{{action}}" method="post" enctype="multipart/form-data" class="form-horizontal">
        <fieldset>
          <legend>{{text_your_email}}</legend>
          <div class="form-group required">
            <label class="col-sm-2 control-label" for="input-email">{{entry_email}}</label>
            <div class="col-sm-10">
              <input type="text" name="email" value="" placeholder="{{entry_email}}" id="input-email" class="form-control" />
            </div>
          </div>
        </fieldset>
        {% include 'd_theme_twig/template/partial/d_button_submit.twig' %}
      </form>
    {% include 'd_theme_twig/template/partial/d_layout_close.twig' %}
{{footer}}
