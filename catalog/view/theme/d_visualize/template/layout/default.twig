{{ header }}
    {% block before_wrapper %}{% endblock %}
    {% block main_wrapper %}
        <div class="main-wrapper" data-slideout-ignore>
            {% block before_content %}
                {% include partial.breadcrumb.template with partial.breadcrumb %}
                {% include partial.d_notification.template with partial.d_notification %}
            {% endblock %}
            <div class="container">
                <div class="row">{{ column_left }}
                    {% if column_left and column_right %}
                        {% set class = 'col-sm-6' %}
                    {% elseif column_left or column_right %}
                        {% set class = 'col-sm-9' %}
                    {% else %}
                        {% set class = 'col-sm-12' %}
                    {% endif %}
                    <main id="content" class="{{ class }}">
                        {{ content_top }}
                                {% block content %}{% endblock %}
                        {{ content_bottom }}
                    </main>
                    {{ column_right }}
                </div>
            </div>
            {% block after_content %}{% endblock %}
        </div>
    {% endblock %}
{{ footer }}

