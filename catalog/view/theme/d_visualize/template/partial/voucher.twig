<div class="panel panel-default">
    <div class="panel-heading">
        <h4 class="panel-title"><a href="#collapse-voucher" data-toggle="collapse" data-parent="#accordion" class="accordion-toggle">{{heading_title}} <i class="fa fa-caret-down"></i></a></h4>
    </div>
    <div id="collapse-voucher" class="panel-collapse collapse">
            <div class="panel-body">
                {% include component.form_group.template with component.form_group|merge({
                    type:'input_group',
                    text_input:entry_voucher,
                    input_group:{
                        input:{
                            type:'text',
                            id:'input-voucher',
                            name:'voucher',
                            value:voucher,
                            placeholder:entry_voucher,
                            classes:'',
                            text_input:entry_voucher,
                        },
                        button:{
                            type:'button',
                            value:button_voucher,
                            id:'button-voucher',
                            style:'primary'
                        }
                    }
                }) %}
            </div>
    </div>
    <script type="text/javascript"><!--
		$(document).ready(function() {
			Total_voucher.init();
		});
		//--></script>

</div>
